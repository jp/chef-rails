{
  "name": "ruby",
  "chef_type": "role",
  "description": "Ruby with rbenv",
  "run_list": [
    "recipe[apt]",
    "recipe[ruby_build]",
    "recipe[rbenv::system_install]",
    "recipe[rbenv::system]",
    "recipe[rbenv]"
  ],
  "default_attributes": {},
