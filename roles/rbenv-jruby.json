{
  "name": "rbenv-jruby",
  "chef_type": "role",
  "description": "JRuby with rbenv",
  "run_list": [
    "recipe[apt]",
    "recipe[ruby_build]",
    "role[java]",
    "recipe[rbenv::system_install]",
    "recipe[rbenv::system]",
    "recipe[rbenv]"
  ],
  "default_attributes": {},
  "override_attributes": {
    "rbenv": {
      "rubies":["jruby-1.7.10"],
      "gems":{
        "jruby-1.7.10":[
          {"name":"bundler"}
        ]
      },
      "global":"jruby-1.7.10"
    }
  }
}
